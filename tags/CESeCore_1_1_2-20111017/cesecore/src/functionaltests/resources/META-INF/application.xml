<?xml version="1.0" encoding="UTF-8"?>
<application version="5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                        http://java.sun.com/xml/ns/javaee/application_5.xsd">

	<description>The EJBCA module consists of the CA beans and the web applications for enrollment, distribution and administration.</description>
	<display-name>EJBCA</display-name>

	<module>
		<ejb>cesecore-ejb_${version}.jar</ejb>
	</module>
	<module>
		<ejb>cesecore-test-ejb.jar</ejb>
	</module>

	<module>
		<web>
			<web-uri>cesecore.war</web-uri>
			<context-root>/cesecore</context-root>
		</web>
	</module>

</application>

